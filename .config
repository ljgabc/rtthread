
#
# app configuration
#

#
# bsp configuration
#
# CONFIG_BSP_TEMPLATE is not set
CONFIG_BSP_N32G003=y

#
# bsp n32g003 configuration
#
# CONFIG_SOC_N32G003F5Q7 is not set
CONFIG_SOC_N32G003F5S7=y
CONFIG_BSP_UART1=y
# CONFIG_N32G003_UART1_TX_PA2 is not set
# CONFIG_N32G003_UART1_TX_PA14 is not set
CONFIG_N32G003_UART1_TX_PB0=y
# CONFIG_N32G003_UART1_RX_PA3 is not set
# CONFIG_N32G003_UART1_RX_PA12 is not set
CONFIG_N32G003_UART1_RX_PB1=y
CONFIG_BSP_UART2=y
CONFIG_N32G003_UART2_TX_PA2=y
# CONFIG_N32G003_UART2_TX_PA8 is not set
# CONFIG_N32G003_UART2_TX_PA9 is not set
CONFIG_N32G003_UART2_RX_PA1=y
# CONFIG_N32G003_UART2_RX_PA7 is not set
# CONFIG_N32G003_UART2_RX_PA9 is not set
CONFIG_BSP_I2C1=y
# CONFIG_N32G003_I2C1_SCL_PA2 is not set
CONFIG_N32G003_I2C1_SCL_PA4=y
# CONFIG_N32G003_I2C1_SCL_PA9 is not set
# CONFIG_N32G003_I2C1_SDA_PA1 is not set
# CONFIG_N32G003_I2C1_SDA_PA3 is not set
CONFIG_N32G003_I2C1_SDA_PA5=y
# CONFIG_N32G003_I2C1_SDA_PA8 is not set
# end of bsp n32g003 configuration
# end of bsp configuration

#
# component configuration
#

#
# device subsystem
#
CONFIG_USE_DEVICE=y
CONFIG_USE_ADC=y
CONFIG_USE_I2C=y
CONFIG_USE_PIN=y
# CONFIG_USE_PWM is not set
CONFIG_USE_SERIAL=y
CONFIG_SERIAL_RX_FIFOSZ=64
CONFIG_SERIAL_TX_FIFOSZ=64
# CONFIG_SERIAL_USE_DMA_RX is not set
# CONFIG_SERIAL_USE_DMA_TX is not set
# CONFIG_USE_SPI is not set
# CONFIG_USE_WDT is not set
# CONFIG_USE_TIM is not set
# CONFIG_USE_FLASH is not set
# CONFIG_USE_RTC is not set
# end of device subsystem

#
# ipc subsystem
#
CONFIG_USE_RINGBUFFER=y
CONFIG_USE_COMPLETION=y
CONFIG_USE_DATAQUEUE=y
CONFIG_USE_WORKQUEUE=y
# end of ipc subsystem
# end of component configuration

#
# cpu configuration
#
CONFIG_ARCH_ARM=y
# CONFIG_ARCH_RISCV is not set
CONFIG_ARM_CM0=y
# CONFIG_ARM_CM3 is not set
# CONFIG_ARM_CM4 is not set
# CONFIG_ARM_CM7 is not set
# CONFIG_ARM_CM23 is not set
# CONFIG_ARM_CM33 is not set
# end of cpu configuration

#
# os configuration
#
CONFIG_USE_RTOS=y
CONFIG_OS_TICK_RATE=100
CONFIG_OS_MAX_PRIORITY=8
CONFIG_OS_USE_TIMER=y
CONFIG_OS_TIMER_TASK_PRIORITY=8
CONFIG_OS_TIMER_TASK_STACK_SIZE=256
CONFIG_OS_USE_SEMAPHORE=y
CONFIG_OS_USE_MUTEX=y
CONFIG_OS_USE_MESSAGEQUEUE=y
CONFIG_USE_COMPONENTS_INIT=y
CONFIG_USE_MEMORY_MANAGEMENT=y
CONFIG_HEAP_SIZE=1024
# end of os configuration
